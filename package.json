{
  "name": "project-initializer",
  "version": "1.0.0",
  "description": "TypeScript-based project initializer with PM workflow",
  "main": "dist/init-project.js",
  "bin": {
    "init-project": "./dist/init-project.js",
    "project-init": "./dist/init-project.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/init-project.js",
    "dev": "ts-node src/init-project.ts",
    "install-global": "npm run build && npm install -g .",
    "uninstall-global": "npm uninstall -g project-initializer"
  },
  "keywords": ["project", "initializer", "typescript", "pm", "workflow"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0"
  }
}
